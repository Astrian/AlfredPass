				<key>script</key>
				<string>function run(argv) {
  let props = argv[0].split("\n")
  let password = props[0]
  let username = ""
  for (let i in props) {
    if (props[i].startsWith("login: ")) {
      username = props[i].split("login: ")[1]
    }
  }
  
  return `${username}\n${password}`
}</string>
